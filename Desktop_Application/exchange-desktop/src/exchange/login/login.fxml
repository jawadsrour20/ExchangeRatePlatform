<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<FlowPane alignment="CENTER" orientation="VERTICAL" prefHeight="400.0" prefWidth="600.0" styleClass="root" stylesheets="/exchange/login/login.css" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="exchange.login.Login">
    <Label styleClass="title" text="Login">
      <font>
         <Font size="32.0" />
      </font></Label>
    <Label styleClass="label" text="Username">
      <font>
         <Font size="15.0" />
      </font></Label>
    <TextField fx:id="usernameTextField" />
    <Label styleClass="label" text="Password">
      <font>
         <Font size="15.0" />
      </font></Label>
    <TextField fx:id="passwordTextField" />
    <Button onAction="#login" prefHeight="25.0" prefWidth="184.0" styleClass="login-btn" text="Login" />
</FlowPane>
